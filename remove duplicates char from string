package test;
import java.util.Scanner;
public class dupstring {
	public static void main(String[]args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		String news="";
		char c;
		for(int i=0;i<s.length();i++) {
			c=s.charAt(i);
			if(news.indexOf(c)==-1) {
				news+=c;
			}
		}
			System.out.print(news);
	}
}
